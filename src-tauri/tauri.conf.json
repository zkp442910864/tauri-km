{
    "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
    "productName": "tauri-km",
    "version": "0.1.0",
    "identifier": "com.tauri.tauri-km",
    "build": {
        "frontendDist": "../build",
        "devUrl": "http://localhost:5173",
        "beforeDevCommand": "pnpm dev",
        "beforeBuildCommand": "pnpm build"
    },
    "app": {
        "windows": [
            {
                "title": "tauri-km",
                "width": 960,
                "height": 960,
                "resizable": true,
                "fullscreen": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "plugins": {
        "sql": {}
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}